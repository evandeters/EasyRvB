$TTL 604800
@ IN SOA primary.[[ domain ]]. root.primary.linuxtechi.local. (
                                   2022072651 ; Serial
                                   3600 ; Refresh
                                   1800 ; Retry
                                   604800 ; Expire
                                   604600 ) ; Negative Cache TTL
;Name Server Information
@       IN  NS    primary.[[ domain ]].

;IP address of Your Domain Name Server(DNS)
primary IN  A     [[ dns_ip ]]

;A Record for Host names
{{ range . }}
{{ .Hostname }} IN A {{ .Ip }}
{{ end }}
